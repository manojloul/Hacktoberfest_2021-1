#include<bits/stdc++.h>
using namespace std;
class Solution{
public:
	void merge(int arr1[], int arr2[], int n, int m) {
	    // code here
	    int i=0;
	    int j=0;
	    int k=n-1;
	    while(i<=k && j<m)
	    {
	        if(arr1[i]<arr2[j])
	        i++;
	        else
	        {
	            int temp=arr1[k];
	            arr1[k]=arr2[j];
	            arr2[j]=temp;
	            k--;
	            j++;
	        }
	    }
	    sort(arr1,arr1+n);
	    sort(arr2,arr2+m);
	}
};
 int main()
 {
  int t;
  cin>>t;
  while(t--)
  {
    int n,m,l;
    cin>>n>>m;
    int arr[n],arr2[m];
    
    for(i=0;i<n;++i)
    {
       cin>>arr[i];
    }
    for(i=0;i<m;++i)
    {
       cin>>arr2[i];
    }
    Solution ob;
    ob.merge(arr,arr2,n,m);
    for(i=0;i<n;++i)
    {
       cout<<arr[i]<<" ";
    }
    for(i=0;i<m;++i)
    {
       cout<<arr2[i]<<" ";
    }
    cout<<endl;
  }
  return 0;
  }
