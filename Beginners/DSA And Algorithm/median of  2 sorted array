#include<<bits/stdc++.h>
using nnamespace std;
double MedianOfArrays(vector<int>& array1, vector<int>& array2);

class Solution{
    public:
    double MedianOfArrays(vector<int>& array1, vector<int>& array2)
    {
    
    vector<int> v;
    for(int i=0;i<array1.size();++i)
    v.push_back(array1[i]);
    for(int i=0;i<array2.size();++i)
    v.push_back(array2[i]);
    
    sort(v.begin(),v.end());
   
    int m=v.size();
    if(m%2!=0)
    return (double)v[m/2];
    else
    return (double)(v[(m-1)/2]+v[m/2])/2.0;
    }
};
int main()
{
int t;
cin>>t;
while(t--)
{
  int m,n;
  cin>>m;
  vector<int> arr(m);
  for(int i=0;i<m;++i)
  cin>>arr[i];
  cin>>n;
  
  vector<int> arr2(m);
  for(int i=0;i<n;++i)
  cin>>arr2[i];
  
  Solution ob;
  cout<<ob.MedianOfArrays(arr1,arr2)<<endl;
  }
return 0;
}
